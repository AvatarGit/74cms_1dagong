<script>
jQuery(function($){
	$(".baoming_btn").click(function(){
		if($("#alert01").is(":visible")){
			$("#alert01").hide();
		}
		var obj = $("#personal");
		obj.fadeIn();

	});
	$(".baoming_btn02").click(function(){
		if($("#personal").is(":visible")){
			$("#personal").hide();
		}
		var obj = $("#alert01");
		obj.fadeIn();
	});
	$(".close_btn").click(function(){
		$(this).parents(".winpop").fadeOut();	
	})
})
</script>

